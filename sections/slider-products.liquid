        <div class="product">
            <picture>
                <img src="{{ product.featured_image.src | img_url: 'master' }}" alt="ss">
            </picture>
            <div class="detail">
                <p>
                    <a href="{{ product. }}">{{ product.title }}</a><br>
                    <samp>{{ product.price | money }}</samp>
                </p>
            </div>
        </div>
